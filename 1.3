#include<stdio.h>

int main()
{
	int a[10];
	int i, max, min;
	double aver = 0;
	for (i = 0;i < 10;i++)
	{
		a[i] = rand ()% 71 + 30;
		printf("%d ", a[i]);
		aver += a[i];
	}
	max = min = a[0];
	for (i = 1;i < 10;i++)
	{
		if (a[i] > max) max = a[i];
		if (a[i] < min) min = a[i];
	}
	aver /= 10;
	printf("min=%d max=%d aver=%f", min, max, aver);
	return 0;

}
