//选择排序从小到大
#include<stdio.h>
#define N 6
int main()
{
	int a[N];
	int i;
	for (i = 0;i < N;i++)
	{
		a[i] = rand() % 100;
		printf("%d ", a[i]);
	}
	int j;
	int t;
	int min;
	for (i = 0;i < N - 1;i++)
	{
		min = i;
		for (j = i + 1;j < N;j++)
			if (a[min] > a[j]) min = j;
				t = a[i];
				a[i] = a[min];
				a[min] = t;
	}
	printf("排序之后：");
	for (i = 0;i < N;i++)
		printf("%d ", a[i]);
	return 0;
}
