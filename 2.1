#include<stdio.h>
#define N 6
int main()
{
	int a[N];
	int i;
	for (i = 0;i < N;i++)
	{
		a[i] = rand() % 50;
		printf("%d ", a[i]);
	}
	int j;                                 /* 从小到大冒泡排序*/
	for (i = 0;i < N - 1;i++)
	{
		for (j = 0;j < N - 1 - i;j++)
		{
			if (a[j] > a[j + 1])
			{
				int t = a[j];
				a[j] = a[j + 1];
				a[j + 1] = t;
			}
		}
	}
	printf("之后：\n");
	for (i = 0;i < N;i++)
		printf(" %d ", a[i]);
	return 0;
}
