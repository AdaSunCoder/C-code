#include<stdio.h>
#include<string.h>
int main()
{
	char s[20],a[20],b[20];
	int i,l,j=0,k=0,p=0;
	scanf("%s",s);
	l=strlen(s);
	for(i=0;i<(l/2);i++)
	{
		strcpy(a[j],s[i]);
		j++;
	}
	for(i=l-1;i>(l/2);i--)
	{
		strcpy(b[k],s[i]);
		k++;
	}
	for(j=0,k=0;j<(1/2),k<(l/2);j++,k++)
	{
		if(a[j]==b[k])  p++;
    }
    if(p==(l/2)) printf("YES");
    else  printf("NO");
    return 0;
}
