
#include<stdio.h>
int path(int n)
{
	if (n == 2) return 1;
	else if (n == 3) return 2;
	else if (n == 4) return 3;
	else if (n > 4 && n % 2 != 0) return path(n - 1) +path (n - 2);
	else return path(n - 1) + path(n - 2) + path(n - 3);
}
int main()
{
	int n;
	scanf_s("%d", &n);
	printf("%d", path(n));
	return 0;
}
